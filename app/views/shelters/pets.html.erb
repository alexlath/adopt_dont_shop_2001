<main>
  <h2><%= "#{@shelter.name} Pets" %></h2>
  <% @shelter.pets.each do |pet| %>
    <%= image_tag "#{pet.image}" %><br/>
    <h1><%= pet.name %></h1>
    <p><%= "Approximate Age: #{pet.approx_age}" %></p>
    <p><%= "Sex: #{pet.sex}" %></p>
    <p><%= "Status: #{pet.status}" %></p>
  <% end %>
</main>